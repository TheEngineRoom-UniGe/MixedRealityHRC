<launch>

  <!-- Limb Argument -->
  <arg name="limb" default="left" />

  <!-- Start the Joint Trajectory Action Server -->
  <node name="rsdk_joint_trajectory_action_server" pkg="baxter_interface"
  type="joint_trajectory_action_server.py" args="--limb $(arg limb) --mode position"
  required="true" />

  <!-- Run the Joint Trajectory Action -->
  <node name="rsdk_joint_trajectory" pkg="baxter_moveit"
  type="joint_trajectory_client.py" args="--limb $(arg limb)"
  output="screen" />
  
  <node name="gripper_action_server" pkg="baxter_interface"
  type="gripper_action_server.py"
  output="screen" />
  
</launch>
